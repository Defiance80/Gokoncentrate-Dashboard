{
  "name": "nasirkhan/laravel-starter",
  "description": "A CMS like modular Laravel starter project.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "GPL-3.0-or-later",
  "type": "project",
  "require": {
    "php": "^8.2",
    "bangnokia/laravel-bunny-storage": "^2.0",
    "barryvdh/laravel-dompdf": "^3.1",
    "dompdf/dompdf": "^3.1",
    "giggsey/libphonenumber-for-php": "^8.13",
    "google/apiclient": "^2.15",
    "guzzlehttp/guzzle": "^7.8",
    "jenssegers/agent": "^2.6",
    "kreait/laravel-firebase": "^6.1",
    "laracasts/flash": "^3.2",
    "laravel/framework": "^12.0",
    "laravel/sanctum": "^4.0",
    "laravel/socialite": "^5.16",
    "laravel/tinker": "^2.8",
    "lavary/laravel-menu": "^1.8",
    "league/csv": "^9.15",
    "league/flysystem-aws-s3-v3": "^3.28",
    "livewire/livewire": "^3.2",
    "maatwebsite/excel": "^3.1",
    "mews/purifier": "^3.4",
    "midtrans/midtrans-php": "^2.6",
    "nasirkhan/module-manager": "^1.3",
    "nwidart/laravel-modules": "^10.0",
    "paragonie/sodium_compat_ext_sodium": "^1.0",
    "paypal/paypal-checkout-sdk": "^1.0",
    "php-ai/php-ml": "^0.10.0",
    "platformcommunity/flysystem-bunnycdn": "^3.3",
    "predis/predis": "^2.2",
    "propaganistas/laravel-phone": "^5.3",
    "razorpay/razorpay": "^2.9",
    "simplesoftwareio/simple-qrcode": "^4.2",
    "spatie/laravel-activitylog": "^4.8",
    "spatie/laravel-html": "^3.3",
    "spatie/laravel-medialibrary": "^11.15",
    "spatie/laravel-permission": "^6.1",
    "spatie/laravel-webhook-client": "^3.2",
    "spatie/laravel-webhook-server": "^3.8",
    "stichoza/google-translate-php": "^5.2",
    "stripe/stripe-php": "^15.9",
    "symfony/http-foundation": "^7.0",
    "tinymce/tinymce": "^7.2",
    "web-token/jwt-library": "^3.4",
    "yajra/laravel-datatables-oracle": "^12.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/breeze": "^2.1",
    "laravel/pint": "^1.0",
    "laravel/telescope": "^5.5",
    "mockery/mockery": "^1.4.4",
    "phpunit/phpunit": "^10.1",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/"
    },
    "classmap": [
      "app/Models"
    ],
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "clear-all": [
      "composer dumpautoload -o",
      "@php artisan clear-compiled",
      "@php artisan cache:clear",
      "@php artisan route:clear",
      "@php artisan view:clear",
      "@php artisan config:clear",
      "@php artisan cache:forget spatie.permission.cache"
    ],
    "fix-cs-dry": [
      "vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
    ],
    "fix-cs": [
      "vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope",
        "barryvdh/laravel-ide-helper"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
